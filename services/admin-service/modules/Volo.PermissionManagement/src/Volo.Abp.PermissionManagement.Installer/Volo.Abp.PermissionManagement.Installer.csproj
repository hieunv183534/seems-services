        <PackagePath>content\</PackagePath>
      </Content>
      <None Remove="..\..\**\*.abppkg*" />
      <Content Include="..\..\**\*.abppkg*">
        <Pack>true</Pack>
        <PackagePath>content\</PackagePath>
      </Content>
    </ItemGroup>

</Project>
